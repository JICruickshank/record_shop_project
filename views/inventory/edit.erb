<p>Edit Album?</p>

<form action="/inventory/<%= @album.id %>" method="post">
  <label for="title">Title: </label>
  <input type="text" name="title" value="<%= @album.title%>">
  <label for="name">Artist: </label>
  <input type="text" name="name" value="<%= @album.artist.name%>">
  <label for="genre">Genre: </label>
  <input type="text" name="genre" value="<%= @album.genre%>">
  <label for="quantity">Quantity: </label>
  <input type="text" name="quantity" value="<%= @album.quantity%>">
  <input type="submit" value="Confirm Changes?">

</form>
